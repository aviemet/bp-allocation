{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "jsx": "react",
    "checkJs": true,
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "/*": [
        "./*"
      ],
    }
  },
  "exclude": [
    "node_modules",
    "**/node_modules/*",
    ".meteor"
  ],
  "typeAcquisition": {
    "include": [
      "meteor"
    ]
  }
}