{
  "name": "bpallocation",
  "private": true,
  "scripts": {
    "start": "meteor run",
    "dev": "ROOT_URL=http://0.0.0.0:3000 meteor --settings privateSettings.json run",
    "visualize": "meteor --production --extra-packages bundle-visualizer",
    "build": "meteor build --server-only --directory ../allocation",
    "lint": "eslint --ext js,jsx,ts,tsx .",
    "lint:fix": "eslint --ext js,jsx,ts,tsx . --fix",
    "test": "meteor test --once --driver-package meteortesting:mocha --port 3333",
    "test:watch": "meteor test --driver-package meteortesting:mocha --port 3333",
    "test:app": "TEST_WATCH=1 meteor test --full-app --driver-package meteortesting:mocha --port 3333",
    "test:server": "TEST_WATCH=1 meteor test --full-app --driver-package meteortesting:mocha --port 3333 TEST_CLIENT=0",
    "test:client": "TEST_WATCH=1 meteor test --full-app --driver-package meteortesting:mocha --port 3333 TEST_SERVER=0",
    "notnow:posttest": "npm run lint --silent"
  },
  "dependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-syntax-decorators": "^7.8.3",
    "@babel/runtime": "^7.9.6",
    "@ckeditor/ckeditor5-build-balloon": "^19.0.0",
    "@ckeditor/ckeditor5-build-classic": "^19.0.0",
    "@ckeditor/ckeditor5-react": "^2.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "crc-32": "^1.2.0",
    "crc32-browser": "^1.0.0",
    "history": "^4.10.1",
    "libphonenumber-js": "^1.7.31",
    "lodash": "^4.17.15",
    "matter-fireworks": "^1.0.2",
    "matter-js": "^0.14.2",
    "meteor-node-stubs": "^0.4.1",
    "mobx": "^5.15.2",
    "mobx-react-lite": "^1.5.2",
    "moment": "^2.24.0",
    "numeral": "^2.0.6",
    "papaparse": "^4.6.3",
    "react": "^16.12.0",
    "react-countdown-clock": "^2.7.0",
    "react-dom": "^16.12.0",
    "react-image": "^1.5.1",
    "react-input-range": "^1.3.0",
    "react-quill": "^1.3.5",
    "react-router": "^4.3.1",
    "react-router-dom": "^5.1.2",
    "react-rte": "^0.16.1",
    "react-transition-group": "^4.3.0",
    "recharts": "^2.0.0-beta.1",
    "semantic-ui-react": "^0.88.2",
    "simpl-schema": "^1.5.6",
    "styled-components": "^3.4.10",
    "twilio": "^3.39.2"
  },
  "devDependencies": {
    "@babel/types": "^7.8.3",
    "babel-eslint": "^10.0.3",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.18.0",
    "faker": "^4.1.0",
    "jest": "^24.9.0"
  },
  "meteor": {
    "mainModule": {
      "client": "client/main.js",
      "server": "server/main.js"
    }
  },
  "mocha": {
    "recursive": true
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ]
    ]
  }
}
