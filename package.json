{
  "name": "bpallocation",
  "private": true,
  "scripts": {
    "start": "meteor run",
    "dev": "ROOT_URL=http://0.0.0.0:3000 meteor --settings privateSettings.json run",
    "visualize": "meteor --production --extra-packages bundle-visualizer",
    "build": "meteor build --server-only --directory ../allocation",
    "test": "meteor --settings privateSettings.json test --once --driver-package meteortesting:mocha --port 3333",
    "test:watch": "meteor --settings privateSettings.json test --driver-package meteortesting:mocha --port 3333",
    "test:app": "TEST_WATCH=1 TEST_BROWSER_DRIVER=chrome meteor --settings privateSettings.json test --full-app --driver-package meteortesting:mocha --port 3333",
    "test:server": "TEST_WATCH=1 meteor --settings privateSettings.json test --full-app --driver-package meteortesting:mocha --port 3333 TEST_CLIENT=0",
    "test:client": "TEST_WATCH=1 meteor --settings privateSettings.json test --full-app --driver-package meteortesting:mocha --port 3333 TEST_SERVER=0"
  },
  "dependencies": {
    "@artsy/fresnel": "^1.3.1",
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-syntax-decorators": "^7.8.3",
    "@babel/runtime": "^7.9.6",
    "@ckeditor/ckeditor5-alignment": "^25.0.0",
    "@ckeditor/ckeditor5-build-balloon": "^25.0.0",
    "@ckeditor/ckeditor5-build-classic": "^25.0.0",
    "@ckeditor/ckeditor5-react": "^3.0.1",
    "@sendgrid/mail": "^7.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "classnames": "^2.2.6",
    "crc-32": "^1.2.0",
    "crc32-browser": "^1.0.0",
    "libphonenumber-js": "^1.7.31",
    "lodash": "^4.17.20",
    "matter-fireworks": "^1.0.2",
    "matter-js": "^0.16.1",
    "meteor-node-stubs": "^1.0.1",
    "mobx": "^5.15.2",
    "mobx-react-lite": "^1.5.2",
    "moment": "^2.24.0",
    "numeral": "^2.0.6",
    "papaparse": "^4.6.3",
    "react": "^17.0.1",
    "react-countdown-clock": "^2.7.0",
    "react-dom": "^17.0.1",
    "react-image": "^4.0.3",
    "react-input-range": "^1.3.0",
    "react-quill": "^1.3.5",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^4.0.2",
    "react-summernote": "^2.0.0",
    "react-textarea-autosize": "^8.3.1",
    "react-transition-group": "^4.3.0",
    "recharts": "^2.0.0-beta.1",
    "semantic-ui-react": "^2.0.3",
    "simpl-schema": "^1.5.6",
    "styled-components": "^5.2.1",
    "twilio": "^3.39.2"
  },
  "devDependencies": {
    "@babel/types": "^7.8.3",
    "babel-eslint": "^10.0.3",
    "chai": "^4.2.0",
    "chromedriver": "^88.0.0",
    "eslint": "^7.20.0",
    "eslint-plugin-react": "^7.18.0",
    "faker": "^5.4.0",
    "jest": "^26.6.3",
    "selenium-webdriver": "^4.0.0-beta.1"
  },
  "meteor": {
    "mainModule": {
      "client": "client/main.js",
      "server": "server/main.js"
    }
  },
  "mocha": {
    "recursive": true
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ]
    ]
  }
}
