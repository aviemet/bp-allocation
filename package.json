{
  "name": "bpallocation",
  "private": true,
  "scripts": {
    "start": "meteor run",
    "dev": "ROOT_URL=http://batterysf.com:3000 meteor --settings privateSettings.json run",
    "visualize": "meteor --production --extra-packages bundle-visualizer",
    "build": "meteor build --server-only --directory ../allocation",
    "lint": "eslint --ext js,jsx,ts,tsx .",
    "lint:fix": "eslint --ext js,jsx,ts,tsx . --fix",
    "test": "meteor test --once --driver-package meteortesting:mocha --port 3333",
    "test:watch": "meteor test --driver-package meteortesting:mocha --port 3333",
    "test:app": "TEST_WATCH=1 meteor test --full-app --driver-package meteortesting:mocha --port 3333",
    "test:server": "TEST_WATCH=1 meteor test --full-app --driver-package meteortesting:mocha --port 3333 TEST_CLIENT=0",
    "test:client": "TEST_WATCH=1 meteor test --full-app --driver-package meteortesting:mocha --port 3333 TEST_SERVER=0",
    "notnow:posttest": "npm run lint --silent"
  },
  "dependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-syntax-decorators": "^7.2.0",
    "@babel/runtime": "^7.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "history": "^4.7.2",
    "libphonenumber-js": "^1.7.25",
    "lodash": "^4.17.15",
    "meteor-node-stubs": "^0.4.1",
    "mobx": "^5.13.0",
    "mobx-react-lite": "^1.4.1",
    "moment": "^2.24.0",
    "numeral": "^2.0.6",
    "papaparse": "^4.6.3",
    "react": "^16.8.6",
    "react-countdown-clock": "^2.4.0",
    "react-dom": "^16.8.6",
    "react-image": "^1.5.1",
    "react-input-range": "^1.3.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.4.0-beta.8",
    "react-transition-group": "^2.5.0",
    "semantic-ui-react": "^0.82.5",
    "simpl-schema": "^1.5.3",
    "styled-components": "^3.4.10",
    "victory": "^32.2.3"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "chai": "^4.2.0",
    "eslint": "^6.0.1",
    "eslint-plugin-react": "^7.14.2",
    "faker": "^4.1.0",
    "jest": "^24.8.0"
  },
  "meteor": {
    "mainModule": {
      "client": "client/main.js",
      "server": "server/main.js"
    }
  },
  "mocha": {
    "recursive": true
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ]
    ]
  }
}
