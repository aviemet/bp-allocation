{
  "name": "bpallocation",
  "private": true,
  "scripts": {
    "start": "meteor run",
    "dev": "METEOR_OFFLINE_CATALOG=1 ROOT_URL=http://0.0.0.0:3000 meteor --settings privateSettings.json run",
    "visualize": "meteor --production --extra-packages bundle-visualizer",
    "build": "meteor build --server-only --directory ../allocation",
    "test": "meteor --settings privateSettings.json test --once --driver-package meteortesting:mocha --port 3333",
    "test:watch": "meteor --settings privateSettings.json test --driver-package meteortesting:mocha --port 3333",
    "test:app": "TEST_WATCH=1 TEST_BROWSER_DRIVER=chrome meteor --settings privateSettings.json test --full-app --driver-package meteortesting:mocha --port 3333",
    "test:server": "TEST_WATCH=1 meteor --settings privateSettings.json test --full-app --driver-package meteortesting:mocha --port 3333 TEST_CLIENT=0",
    "test:client": "TEST_WATCH=1 meteor --settings privateSettings.json test --full-app --driver-package meteortesting:mocha --port 3333 TEST_SERVER=0",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "generate-types": "GENERATE_TYPES=1 ts-node --project scripts/tsconfig.json scripts/generate-types-real.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "@sendgrid/mail": "^8.1.6",
    "@tanstack/react-router": "^1.133.36",
    "@tiptap/core": "^3.10.1",
    "@tiptap/extension-font-family": "^3.10.1",
    "@tiptap/extension-heading": "^3.10.1",
    "@tiptap/extension-image": "^3.10.1",
    "@tiptap/extension-link": "^3.10.1",
    "@tiptap/extension-placeholder": "^3.10.1",
    "@tiptap/extension-strike": "^3.10.1",
    "@tiptap/extension-table": "^3.10.1",
    "@tiptap/extension-text-align": "^3.10.1",
    "@tiptap/extension-text-style": "^3.10.1",
    "@tiptap/extension-underline": "^3.10.1",
    "@tiptap/pm": "^3.10.1",
    "@tiptap/react": "^3.10.1",
    "@tiptap/starter-kit": "^3.10.1",
    "classnames": "^2.5.1",
    "date-fns": "^4.1.0",
    "immutable": "^5.1.4",
    "libphonenumber-js": "^1.12.25",
    "lodash": "^4.17.21",
    "matter-fireworks": "^1.0.2",
    "matter-js": "^0.20.0",
    "meteor-node-stubs": "^1.2.24",
    "mobx": "^6.15.0",
    "mobx-react-lite": "^4.1.1",
    "mui-tiptap": "^1.28.1",
    "notistack": "^3.0.2",
    "numeral": "^2.0.6",
    "papaparse": "^5.5.3",
    "react": "^19.2.0",
    "react-countdown-circle-timer": "^3.2.1",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.65.0",
    "react-image": "^4.1.0",
    "react-textarea-autosize": "^8.5.9",
    "react-transition-group": "^4.4.5",
    "simpl-schema": "^3.4.6",
    "twilio": "^5.10.4"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/eslint-parser": "^7.28.5",
    "@babel/plugin-transform-class-properties": "^7.27.1",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/runtime": "^7.28.4",
    "@babel/types": "^7.28.5",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.38.0",
    "@eslint/json": "^0.13.2",
    "@faker-js/faker": "^10.1.0",
    "@stylistic/eslint-plugin": "^5.5.0",
    "@types/chai": "^5.2.3",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/meteor": "^2.9.10",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.9.2",
    "@types/numeral": "^2.0.5",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/react-slider": "^1.3.6",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "chai": "^6.2.0",
    "chromedriver": "^142.0.0",
    "commitizen": "^4.3.1",
    "commitlint": "^20.1.0",
    "eslint": "^9.38.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsonc": "^2.21.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^16.4.0",
    "instead": "^1.0.3",
    "jest": "^30.2.0",
    "schema-to-types": "^1.2.4",
    "selenium-webdriver": "^4.38.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "meteor": {
    "modern": true
  },
  "mocha": {
    "recursive": true
  },
  "browserslist": [
    "defaults"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint:types && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "npm run lint:fix"
    ],
    "*.{md,json,yml}": [
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
